{
  "compilerOptions": {
    "target": "ES2017",
    "sourceMap": true,
    "inlineSourceMap": false,
    "inlineSources": false,
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "module": "esnext",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ],
    "strictNullChecks": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "next-env.d.ts",
    "out/types/**/*.ts",
    ".next/types/**/*.ts",
    "components/Services.tsx",
    "index.ts",
    "lib/prisma.js",
    "prisma/elys.js"
  ],
  "exclude": [
    "node_modules"
  ]
}